@model RegisterModel

@{
    Layout = "_AccLayout";
    ViewData["Title"] = "Register";
}

<div class="d-lg-none text-center p-5 pt-10">
    <a href="~/" class="mb-15">
        <img alt="Logo" src="~/img/logos/logo.png" class="h-80px" />
    </a>
</div>

<div class="d-flex flex-column flex-lg-row flex-column-fluid stepper stepper-pills stepper-column" id="signup_stepper">
    <div class="d-none d-lg-flex flex-column flex-lg-row-auto w-xl-500px bg-lighten shadow-sm">
        <div class="d-flex flex-column position-xl-fixed top-0 bottom-0 w-xl-500px scroll-y">
            <div class="d-flex flex-row-fluid flex-column flex-center p-10 pt-lg-20">
                <input id="hd_checkemailurl" type="hidden" value="@Model.CheckEmailUrl" />
                <input id="hd_generatevalidateemailurl" type="hidden" value="@Model.GenerateValidateEmailUrl" />
                <input id="hd_validateemailurl" type="hidden" value="@Model.ValidateEmailUrl" />
                <a asp-controller="Home" asp-action="Index" class="mb-10 mb-lg-20"> <img alt="Logo" src="~/img/logos/logo.png" class="h-80px" /> </a>
                <div class="stepper-nav">
                    <div class="stepper-item current" data-kt-stepper-element="nav">
                        <div class="stepper-line w-40px"></div>
                        <div class="stepper-icon w-40px h-40px">
                            <i class="stepper-check fas fa-check"></i>
                            <span class="stepper-number">1</span>
                        </div>
                        <div class="stepper-label">
                            <h3 class="stepper-title">Account Type</h3>
                            <div class="stepper-desc fw-bold">Choose Your Account Type</div>
                        </div>
                    </div>
                    <div class="stepper-item" data-kt-stepper-element="nav">
                        <div class="stepper-line w-40px"></div>
                        <div class="stepper-icon w-40px h-40px">
                            <i class="stepper-check fas fa-check"></i>
                            <span class="stepper-number">2</span>
                        </div>
                        <div class="stepper-label">
                            <h3 class="stepper-title">Basic Information</h3>
                            <div class="stepper-desc fw-bold">Setup Your Account Settings</div>
                        </div>
                    </div>
                    <div class="stepper-item" data-kt-stepper-element="nav">
                        <div class="stepper-line w-40px"></div>
                        <div class="stepper-icon w-40px h-40px">
                            <i class="stepper-check fas fa-check"></i>
                            <span class="stepper-number">3</span>
                        </div>
                        <div class="stepper-label">
                            <h3 class="stepper-title">Address Information</h3>
                            <div class="stepper-desc fw-bold">Your Business Related Info</div>
                        </div>
                    </div>
                    <div class="stepper-item" data-kt-stepper-element="nav">
                        <div class="stepper-line w-40px"></div>
                        <div class="stepper-icon w-40px h-40px">
                            <i class="stepper-check fas fa-check"></i>
                            <span class="stepper-number">4</span>
                        </div>
                        <div class="stepper-label">
                            <h3 class="stepper-title">Validate Account</h3>
                            <div class="stepper-desc fw-bold">Set Your Payment Methods</div>
                        </div>
                    </div>
                    <div class="stepper-item" data-kt-stepper-element="nav">
                        <div class="stepper-line w-40px"></div>
                        <div class="stepper-icon w-40px h-40px">
                            <i class="stepper-check fas fa-check"></i>
                            <span class="stepper-number">5</span>
                        </div>
                        <div class="stepper-label">
                            <h3 class="stepper-title">Finalize</h3>
                            <div class="stepper-desc fw-bold">Review &amp; choose password</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="d-flex flex-row-auto bgi-no-repeat bgi-position-x-center bgi-size-contain bgi-position-y-bottom min-h-150px min-h-lg-300px" style="background-image: url(assets/media/illustrations/coding.png)"></div>
        </div>
    </div>
